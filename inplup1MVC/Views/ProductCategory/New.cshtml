@model inplup1MVC.ViewModels.ProductCategoryNewViewModel


@{
    ViewData["Title"] = "Ny produktkategori";
}



<section class="py-5">
    <div class="row">
        <div class="col-lg-12 mb-5">
            <div class="card">
                <div class="card-header">
                    <h3 class="h6 text-uppercase mb-0">New Vaccin</h3>
                </div>

                <div class="card-body">
                    <form asp-controller="ProductCategory" asp-action="New" method="post">
                        <div class="form-group">
                            <label asp-for="Namn">Namn</label>
                            <input class="form-control" asp-for="Namn">
                        </div>
                       


                        <button type="submit" class="btn btn-primary">Submit</button>
                    </form>
                </div>

            </div>

        </div>
    </div>
</section>



